<button class="bg-timber-green-500 text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline">
    {% if lIcon is defined %}
    {% include 'icons/' ~ lIcon %}
    {% endif %}
    <span>{{ text|default('') }}</span>
    {% if rIcon is defined %}
    {% include 'icons/' ~ rIcon %}
    {% endif %}
</button>
